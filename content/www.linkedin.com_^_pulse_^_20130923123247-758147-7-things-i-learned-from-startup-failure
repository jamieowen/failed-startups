_nS('com.linkedin.pulse.controllers.AnalyticsController');_root.com.linkedin.pulse.controllers.AnalyticsController.getAnalytics=function(articleId,isMock,asJson){return _wA({method:"GET",url:"/pulse/"+"author/analytics"+_qS([(articleId==null?null:(function(k,v){return v!=null?(function(k,v){return encodeURIComponent(k)+'='+encodeURIComponent(v)})(k,v):''})("articleId",articleId)),(isMock==null?null:(function(k,v){return k+'='+(!!v)})("isMock",isMock)),(asJson==null?null:(function(k,v){return k+'='+(!!v)})("asJson",asJson))])})}})(jsRoutes);root.play=root.play||{};root.play.jsRoutes=jsRoutes;})(this);</script><meta name="viewport" content="initial-scale=1, maximum-scale=1"><div class="banner-container"><div class="ad-text-link"><iframe id="dexterTopAd" src="about:blank" frameborder="0" width="974" height="17" scrolling="no" data-src="/csp/dtag?sz=1x1&ti=1&p=1&z=pulse-web-article-load&_x="></iframe></div></div><div id="main" class="social-ui"><div class="stream-container" id="stream-app" data-category="channel" data-category-id=""><div class="stream-header" data-paging-context-id="5db5bd1d-cc7a-4ee0-84b6-dd4b9baaf28f"><div class="wrapper"><button id="channels-icon" aria-describedby="channels-dropdown-container"><svg class="icon"><use xlink:href="#hamburger-icon"></use></svg><span class="screen-reader-text">Pulse Channels</span></button><div class="tooltip-content-container"><div id="channels-dropdown-container" role="navigation"></div></div><h3 class="title"><a href="/today/" id="pulse-title">Pulse</a><span class="channel-title"></span></h3><div class="article-actions"></div></div></div><div class="stream-sidebar "><div class="stream-nav-container"><ul class="stream-nav stream-nav-opacity-control" role="tablist" data-paging-start="20" data-paging-count="20" data-paging-token="" data-paging-context-id="5db5bd1d-cc7a-4ee0-84b6-dd4b9baaf28f"><li class="nav-item" role="tab" aria-controls="article-container-5787886290825404488" id="nav-item-5787886290825404488" data-stream